{
  "name": "w3champions-twitch",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "packDist": "yarn build && cd dist && zip -r ../dist.zip ./*",
    "packSrc": "zip -r src.zip index.html favicon.ico src .browserslistrc .eslintignore .eslintrc.js .gitignore package.json README.md tsconfig.json vue.config.js w3champs-live.json yarn.lock",
    "lint": "eslint --ext .js,.vue,.ts --ignore-path .gitignore --fix src"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "vue": "^3.5.12"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "eslint": "^9.14.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.30.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "sass": "^1.80.5",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-plugin-mkcert": "^1.17.6"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
